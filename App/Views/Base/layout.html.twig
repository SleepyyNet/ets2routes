<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" type="text/css" href="{{ css('ets2routes.css') }}">
        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
              integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
        <title>{% block title %}{% endblock %}</title>
    </head>

    <body class="container-fluid">
        {% include '@App/Base/header.html.twig' %}

        {% if flashType() == 'error' %}
            <div class="alert alert-danger col-md-6 col-md-offset-3">{{ flash() }}</div>
        {% elseif flashType() == 'success' %}
            <div class="alert alert-success col-md-6 col-md-offset-3">{{ flash() }}</div>
        {% endif %}


        {% block body %}{% endblock %}
        {% include '@App/Base/footer.html.twig' %}
    </body>
</html>
